[DEFAULT]
undercloud_hostname = undercloud.localdomain
cleanup = False
deployment_user = {{ ansible_user }}
undercloud_debug = true
local_interface = eth1
#local_interface = br-ex  Default in test
local_ip = 192.168.{{ ansible_local.tripleo.setup.tripleo_magic_number }}.1/22
local_mtu = 1350
generate_service_certificate = false
container_images_file = {{ ansible_env.HOME }}/local_images.yaml
#container_images_file = /home/zuul/containers-prepare-parameter.yaml  Default in test
#custom_env_files = {{ ansible_env.HOME }}/undercloud-parameters.yaml
#custom_env_files = /home/zuul/undercloud-parameter-defaults.yaml  Default in test
undercloud_public_host = 192.168.{{ ansible_local.tripleo.setup.tripleo_magic_number }}.2
undercloud_admin_host = 192.168.{{ ansible_local.tripleo.setup.tripleo_magic_number }}.3
{% if (ansible_distribution | lower) == 'redhat' %}
container_insecure_registries = {{ redhat_docker_registry }}
undercloud_nameservers = {{ redhat_dns_servers | join(',') }}
undercloud_ntp_servers = {{ redhat_ntp_servers | join(',') }}
{% else %}
undercloud_nameservers = {{ tripleo_dns_servers | join(',') }}
undercloud_ntp_servers = {{ tripleo_ntp_servers | join(',') }}
{% endif %}
#undercloud_nameservers = 127.0.0.1,1.1.1.1  Default in test
undercloud_enable_selinux = False
mtu = 1350

